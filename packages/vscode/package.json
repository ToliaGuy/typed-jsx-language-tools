{
  "name": "typed-jsx-vscode",
  "displayName": "Typed JSX",
  "description": "A VS Code extension for Typed JSX.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "onLanguage:tsx"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "tsx",
        "extensions": [
          ".tsx"
        ]
      }
    ]
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@types/vscode": "^1.104.0",
    "@volar/language-server": "^2.4.23",
    "@volar/vscode": "^2.4.23",
    "server": "link:..\\server",
    "vscode-languageclient": "^9.0.1"
  }
}
