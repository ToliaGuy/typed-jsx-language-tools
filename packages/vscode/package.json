{
  "name": "typed-jsx-vscode",
  "displayName": "Typed JSX",
  "description": "A VS Code extension for Typed JSX.",
  "repository": {
		"type": "git",
		"url": "https://github.com/ToliaGuy/typed-jsx-language-tools.git",
		"directory": "packages/vscode"
	},
	"categories": [
		"Programming Languages"
	],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "onLanguage:html1"
  ],
  "main": "./dist/client.js",
  "scripts": {
		"build": "node scripts/build -- --minify",
		"watch": "npm run build -- --watch",
		"pack": "npm run build && vsce package --pre-release"
	},
  "contributes": {
    "languages": [
      {
        "id": "html1",
        "extensions": [
          ".html1"
        ]
      }
    ]
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@types/vscode": "^1.104.0",
    "@volar/language-server": "^2.4.23",
    "@volar/vscode": "^2.4.23",
    "server": "link:..\\server",
    "vscode-languageclient": "^9.0.1",
    "esbuild": "latest",
    "vsce": "latest"
  }
}